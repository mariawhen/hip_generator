<div class="jumbotron text-center">
  <h2>Sign Up with Name Generator</h2>
</div>
<div class="container"><%= bootstrap_form_for(@user) do |f| %>
    <%= f.email_field :email, placeholder: "Enter your email."%>
    <%= f.password_field :password, placeholder: "Enter a password.", help: "" %>
    <%= f.password_field :password_confirmation, placeholder: "Confirm your password." %>
    <%= f.submit "Sign Up" %>
  <% end %></div>
